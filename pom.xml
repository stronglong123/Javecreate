            <version>1.2.1</version>
            <scope>system</scope>
            <systemPath>${project.basedir}/src/main/libs/generator-1.2.1.jar</systemPath>
        </dependency>
        <dependency>
            <groupId>gui.ava</groupId>
            <artifactId>html2image</artifactId>
